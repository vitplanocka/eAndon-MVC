<script>

    // show the configuration modal for Alarm start details
    function showDefineAlarmStartDetailsModal(statusIndex, statusName, alarmStartText1Structure, alarmStartText2Structure, alarmStartText3Structure) {
        $('#alarmIndex').text(statusIndex);
        $('#alarmName').text(statusName);

        function isChecked(structure) {
            return structure.split('|')[0] === 'ON';
        }

        $('#failureLocationEnabled').prop('checked', isChecked(alarmStartText1Structure));
        $('#failureTypeEnabled').prop('checked', isChecked(alarmStartText2Structure));
        $('#detailsTextFieldEnabled').prop('checked', isChecked(alarmStartText3Structure));

        function setOptions(structure, targetId) {
            const options = structure.split('|').slice(1).join('|');
            $(targetId).val(options);
        }

        setOptions(alarmStartText1Structure, '#failureLocationOptions');
        setOptions(alarmStartText2Structure, '#failureTypeOptions');

        $('#defineAlarmStartDetailsModal').modal('show');
    }

    function saveAndClose() {
        const statusIndex = $('#alarmIndex').text();
        const failureLocationEnabled = $('#failureLocationEnabled').prop('checked');
        const failureTypeEnabled = $('#failureTypeEnabled').prop('checked');
        const detailsTextFieldEnabled = $('#detailsTextFieldEnabled').prop('checked');

        const failureLocationOptions = (failureLocationEnabled ? 'ON' : 'OFF') + '|' + $('#failureLocationOptions').val().replace(/\n/g, '|');
        const failureTypeOptions = (failureTypeEnabled ? 'ON' : 'OFF') + '|' + $('#failureTypeOptions').val().replace(/\n/g, '|');
        const detailsTextOptions = (detailsTextFieldEnabled ? 'ON' : 'OFF') + '|';

        const url = '/Settings/UpdateAlarmStartDetails';
    
        const data = {
            statusIndex: statusIndex,
            failureLocationOptions: failureLocationOptions,
            failureTypeOptions: failureTypeOptions,
            detailsTextOptions: detailsTextOptions
        };

        $.ajax({
            type: 'POST',
            url: url,
            data: data,
            dataType: 'json',
            success: function(response) {
                if (response.success) {
                    console.log('Values updated successfully.');
                    hideDefineAlarmStartDetailsModal();
                    location.reload();
                } else {
                    console.log('An error occurred while updating the values.');
                }
            },
            error: function() {
                console.log('An error occurred while updating the values.');
            }
        });
    }
    
    function hideDefineAlarmStartDetailsModal() {
        $('#alarmIndex').text('');
        $('#alarmName').text('');
        $('#defineAlarmStartDetailsModal').modal('hide');
    }

    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
        $('[id^="iconBtn"]').click(function () {
            var index = $(this).data('index'); // Get the index of the current button from the data attribute
            $('#iconModal').data('index', index); // Store the index in the modal's data attribute
            $('#iconModal').modal('show');
        });

        $('#closeModalBtn').click(function () {
            $('#iconModal').modal('hide');
        });

        $('.list-inline-item').click(function () {
            var index = $('#iconModal').data('index'); // Get the index of the current button from the data attribute
            var icon = $(this).data('icon'); // Get the data-icon attribute of the clicked element
            $('#iconName_' + index).val(icon); // Set the value of the hidden input field to the selected icon name
            $('#iconModal').modal('hide'); // Hide the modal
            $('#iconForm_' + index).submit();// Submit the form when the icon is clicked
        });
    });
</script>

