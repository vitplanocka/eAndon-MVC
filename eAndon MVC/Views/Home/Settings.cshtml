@model List<StatusDefinition>
@inject MyDbContext dbContext

@{
    var language = dbContext.Settings!.Where(s => s.SettingName == "Language").Select(s => s.CurrentSetting).FirstOrDefault();
    var translations = dbContext.Localization!
        .Select(l => new
        {
            l.Id,
            Text = language == "Translation" ? l.Translation : l.English
        })
        .ToList();
}

<!DOCTYPE html>
<html>
<head>

    <meta charset="UTF-8">
    <title>Settings</title>

</head>



<body>
    <div class="container">
        <span class="pageTitle">@translations.FirstOrDefault(t => t.Id == "Settings")?.Text</span>
        <br />
        <h2>&nbsp;&nbsp;@translations.FirstOrDefault(t => t.Id == "WORKCENTERS")?.Text</h2>

        <table class="table" style="border: 2px solid #1C2331; border-collapse: collapse;">
            <thead style="background-color: #D6EFFF; color: #1C2331; border: 2px solid #1C2331;">
                <tr>
                    <th colspan="3" style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "WorkcenterRow")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "WorkcenterIDUnique")?.Text</th>
                    <th colspan="3" text-align: center;>@translations.FirstOrDefault(t => t.Id == "WorkcenterName")?.Text</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var workcenter in ViewBag.Workcenters)
                {
                    <tr style="border: 1px solid #1C2331;">
                        <td>
                            @if (ViewBag.Workcenters.IndexOf(workcenter) > 0)
                            {
                                <form method="post" asp-controller="Settings" asp-action="MoveWorkcenterUp">
                                    <input type="hidden" name="workcenterRow" value="@workcenter.WorkcenterRow" />
                                    <button type="submit" class="btn btn-primary"><i class="fa fa-chevron-up"></i></button>
                                </form>
                            }
                        </td>
                        <td>
                            &nbsp;&nbsp;
                            <span class="workcenter-row">@workcenter.WorkcenterRow</span>
                            &nbsp;&nbsp;
                        </td>
                        <td>
                            @if (ViewBag.Workcenters.IndexOf(workcenter) < ViewBag.Workcenters.Count - 1)
                            {
                                <form method="post" asp-controller="Settings" asp-action="MoveWorkcenterDown">
                                    <input type="hidden" name="workcenterRow" value="@workcenter.WorkcenterRow" />
                                    <button type="submit" class="btn btn-primary"><i class="fa fa-chevron-down"></i></button>
                                </form>
                            }
                        </td>
                        <td style="border: 1px solid #1C2331;">
                            <form method="post" asp-controller="Settings" asp-action="UpdateWorkcenterID">
                                <input type="hidden" name="workcenterRow" value="@workcenter.WorkcenterRow" />
                                <input type="text" name="workcenterID" value="@workcenter.WorkcenterID" />
                                <button type="submit" class="btn btn-primary">@translations.FirstOrDefault(t => t.Id == "SaveID")?.Text</button>
                            </form>
                        </td>
                        <td>
                            <form method="post" asp-controller="Settings" asp-action="UpdateWorkcenterName">
                                <input type="hidden" name="workcenterRow" value="@workcenter.WorkcenterRow" />
                                <input type="text" name="workcenterName" value="@workcenter.WorkcenterName" />
                                <button type="submit" class="btn btn-primary">@translations.FirstOrDefault(t => t.Id == "SaveName")?.Text</button>
                            </form>
                        </td>
                        <td>
                            <form method="post" asp-controller="Settings" asp-action="DeleteWorkcenter">
                                <input type="hidden" name="workcenterRow" value="@workcenter.WorkcenterRow" />
                                <button type="submit" class="btn btn-secondary">@translations.FirstOrDefault(t => t.Id == "DeleteWorkcenter")?.Text</button>
                            </form>
                        </td>
                    </tr>
                }
                <tr>
                    <form method="post" asp-controller="Settings" asp-action="AddWorkcenter">
                    <td></td>
                    <td>
                        &nbsp;&nbsp;
                        <span class="workcenter-row">@(ViewBag.Workcenters.Count + 1)</span>
                        &nbsp;&nbsp;
                    </td>
                    <td></td>
                    <td style="border: 1px solid #1C2331;">
                        <input type="text" name="newWorkcenterID" placeholder="@translations.FirstOrDefault(t => t.Id == "WorkcenterIDUnique")?.Text" />
                    </td>
                    <td>
                        <input type="text" name="newWorkcenterName" placeholder="@translations.FirstOrDefault(t => t.Id == "WorkcenterName")?.Text" />
                    </td>
                    <td>
                        <button type="submit" class="btn btn-primary">@translations.FirstOrDefault(t => t.Id == "AddWorkcenter")?.Text</button>
                    </td>
                    <input type="hidden" name="workcenterRow" value="@(ViewBag.Workcenters.Count + 1)" />
                    </form>
                </tr>


            </tbody>
        </table>


        <h2>&nbsp;&nbsp;@translations.FirstOrDefault(t => t.Id == "ALARMTYPES")?.Text</h2>
        <table class="table" style="border: 2px solid #1C2331;">
            <thead style="background-color: #D6EFFF; color: #1C2331; border: 2px solid #1C2331;">
                <tr>
                    <th colspan="3" style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "AlarmRow")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "AlarmTypeName")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "AlarmTypeIcon")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "AlarmTypeEnabled")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "AlarmDetailsEnabled")?.Text</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var status in Model)
                {
                    <tr style="border: 1px solid #1C2331;">
                        <td>
                            @if (Model.IndexOf(status) > 0)
                            {
                                <form method="post" asp-controller="Settings" asp-action="MoveAlarmTypeUp">
                                    <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                    <button type="submit" class="btn btn-primary">
                                        <i class="fa fa-chevron-up"></i>
                                    </button>
                                </form>
                            }
                        </td>
                        <td>
                            &nbsp;&nbsp;
                            <span class="workcenter-row">@status.StatusRow</span>
                            &nbsp;&nbsp;
                        </td>
                        <td>
                            @if (Model.IndexOf(status) < Model.Count - 1)
                            {
                                <form method="post" asp-controller="Settings" asp-action="MoveAlarmTypeDown">
                                    <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                    <button type="submit" class="btn btn-primary"><i class="fa fa-chevron-down"></i></button>
                                </form>
                            }
                        </td>

                        <td style="border: 1px solid #1C2331;">
                            <form method="post" asp-controller="Settings" asp-action="UpdateAlarmTypeName">
                                <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                <input type="text" name="statusName" value="@status.StatusName" />
                                <button type="submit" class="btn btn-primary">@translations.FirstOrDefault(t => t.Id == "SaveName")?.Text</button>
                            </form>
                        </td>

                        <td style="border: 1px solid #1C2331;">
                            <div class="row">
                                <div class="col-3">
                                    <i class="@status.IconName fa-2x"></i>
                                </div>
                                <div class="col-9">
                                    <form method="post" asp-controller="Settings" asp-action="UpdateAlarmTypeIcon" id="iconForm_@Model.IndexOf(status)">
                                        <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                        <input type="hidden" name="iconName" id="iconName_@Model.IndexOf(status)" value="@status.IconName" />
                                        <button type="button" class="btn btn-primary" id="iconBtn_@Model.IndexOf(status)" data-index="@Model.IndexOf(status)" data-toggle="modal" data-target="#iconModal">@translations.FirstOrDefault(t => t.Id == "ChooseIcon")?.Text</button>
                                    </form>
                                </div>
                            </div>
                        </td>

                        <td style="border: 1px solid #1C2331;">
                            <form method="post" asp-controller="Settings" asp-action="UpdateStatusEnabled">
                                <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                <select name="statusEnabled" onchange="this.form.submit()">
                                    <option value="True" selected="@status.StatusEnabled">@translations.FirstOrDefault(t => t.Id == "Yes")?.Text</option>
                                    <option value="False" selected="@(status.StatusEnabled == false)">@translations.FirstOrDefault(t => t.Id == "No")?.Text</option>
                                </select>
                            </form>
                        </td>
                        <td style="border: 1px solid #1C2331;">
                            <div style="display: flex; align-items: center;">
                                <form method="post" asp-controller="Settings" asp-action="UpdateAlarmDetailsEnabled">
                                    <input type="hidden" name="statusRow" value="@status.StatusRow" />
                                    <select name="statusDetailsEnabled" onchange="this.form.submit()">
                                        <option value="0" selected="@(status.StatusDetailsEnabled == 0)">@translations.FirstOrDefault(t => t.Id == "NoDetailsEnabled")?.Text</option>
                                        <option value="1" selected="@(status.StatusDetailsEnabled == 1)">@translations.FirstOrDefault(t => t.Id == "EnabledForStart")?.Text</option>
                                        <!-- option value="2" selected="@(status.StatusDetailsEnabled == 2)">@translations.FirstOrDefault(t => t.Id == "EnabledForEnd")?.Text</!-->
                                        <!-- option value="3" selected="@(status.StatusDetailsEnabled == 3)">@translations.FirstOrDefault(t => t.Id == "EnabledForStartEnd")?.Text</!-->
                                    </select>
                                </form>
                                @if (status.StatusDetailsEnabled is 1 or 3)
                                {
                                    <i class="fa fa-play" style="margin-left: 10px; cursor: pointer;" data-toggle="tooltip" title="Define alarm start details" data-row="@status.StatusRow" onclick="showDefineAlarmStartDetailsModal(@Model.IndexOf(status), '@status.StatusName', '@ViewBag.StatusList[Model.IndexOf(status)].AlarmStartText1Structure', '@ViewBag.StatusList[Model.IndexOf(status)].AlarmStartText2Structure', '@ViewBag.StatusList[Model.IndexOf(status)].AlarmStartText3Structure')"></i>
                                }
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

        <!-- modal to enter alarm detail configuration -->
        <div class="modal fade" id="defineAlarmStartDetailsModal" tabindex="-1" role="dialog" aria-labelledby="defineAlarmStartDetailsModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <span class="hidden" id="alarmIndex"></span>
                        <h5 class="modal-title" id="defineAlarmStartDetailsModalLabel">@translations.FirstOrDefault(t => t.Id == "DefineAlarmStartDetails")?.Text<span id="alarmName"></span></h5>
                    </div>
                    <div class="modal-body">
                        <table class="table modal-table" style="border: 2px solid #1C2331;">
                            <thead style="background-color: #D6EFFF; color: #1C2331; border: 2px solid #1C2331;">
                                <tr>
                                    <th style="border: 1px solid #1C2331; text-align: center; width: 20%;">@translations.FirstOrDefault(t => t.Id == "TypeOfDetail")?.Text</th>
                                    <th style="border: 1px solid #1C2331; text-align: center; width: 20%;">@translations.FirstOrDefault(t => t.Id == "DetailEnabled")?.Text</th>
                                    <th style="border: 1px solid #1C2331; text-align: center; width: 20%;">@translations.FirstOrDefault(t => t.Id == "OptionsSeparated")?.Text</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td style="border: 1px solid #1C2331;">@translations.FirstOrDefault(t => t.Id == "FailureLocation")?.Text</td>
                                    <td style="border: 1px solid #1C2331;">
                                        <div class="form-check">
                                            <input type="checkbox" class="form-check-input" id="failureLocationEnabled">
                                            <label class="form-check-label" for="failureLocationEnabled">@translations.FirstOrDefault(t => t.Id == "Enabled")?.Text</label>
                                        </div>
                                    </td>
                                    <td style="border: 1px solid #1C2331;">
                                        <textarea class="form-control" id="failureLocationOptions" rows="3"></textarea>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid #1C2331;">@translations.FirstOrDefault(t => t.Id == "FailureType")?.Text</td>
                                    <td style="border: 1px solid #1C2331;">
                                        <div class="form-check">
                                            <input type="checkbox" class="form-check-input" id="failureTypeEnabled">
                                            <label class="form-check-label" for="failureTypeEnabled">@translations.FirstOrDefault(t => t.Id == "Enabled")?.Text</label>
                                        </div>
                                    </td>
                                    <td style="border: 1px solid #1C2331;">
                                        <textarea class="form-control" id="failureTypeOptions" rows="3"></textarea>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid #1C2331;">@translations.FirstOrDefault(t => t.Id == "DetailsFree")?.Text</td>
                                    <td style="border: 1px solid #1C2331;">
                                        <div class="form-check">
                                            <input type="checkbox" class="form-check-input" id="detailsTextFieldEnabled">
                                            <label class="form-check-label" for="detailsTextFieldEnabled">@translations.FirstOrDefault(t => t.Id == "Enabled")?.Text</label>
                                        </div>
                                    </td>
                                    <td style="border: 1px solid #1C2331;">
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" onclick="hideDefineAlarmStartDetailsModal();" data-dismiss="modal">@translations.FirstOrDefault(t => t.Id == "Cancel")?.Text</button>
                        <button type="button" class="btn btn-primary" onclick="saveAndClose()">@translations.FirstOrDefault(t => t.Id == "SaveAndClose")?.Text</button>
                </div>
            </div>
        </div>
    </div>

        
         <p></p>
        <h2>&nbsp;&nbsp;@translations.FirstOrDefault(t => t.Id == "INTERFACESETTINGS")?.Text</h2>

        <table class="table" style="border: 2px solid #1C2331;">
            <thead style="background-color: #D6EFFF; color: #1C2331; border: 2px solid #1C2331;">
                <tr>
                    <th style="border: 1px solid #1C2331; text-align: center;">
                        <span style="float: left;">@translations.FirstOrDefault(t => t.Id == "SettingName")?.Text</span>
                        <form method="post" asp-controller="Settings" asp-action="ResetInterfaceSettings">
                            <button type="submit" class="btn btn-secondary" style="display: block; float: right;">
                                <i class="fa fa-undo"></i> @translations.FirstOrDefault(t => t.Id == "ResetToDefaultSettings")?.Text
                            </button>
                        </form>
                    </th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "CurrentValue")?.Text</th>
                </tr>
            </thead>
            <tbody>


                @foreach (var setting in ViewBag.SettingsList)
                {
                    <tr>
                        <td style="border: 1px solid #1C2331;">
                            <span>@setting.SettingName</span>
                        </td>
                        <td style="border: 1px solid #1C2331;">
                            <form method="post" action="@Url.Action("UpdateSetting", "Settings")">
                                <input type="hidden" name="settingID" value="@setting.SettingID" />
                                <select name="settingValue" onchange="this.form.submit()">
                                    @foreach (var value in setting.PossibleSettings.Split('|'))
                                    {
                                        <option value="@value" selected="@(value == setting.CurrentSetting)">@(value)</option>
                                    }
                                </select>
                            </form>
                        </td>
                    </tr>
                }

            </tbody>
        </table>



        <p></p>
        <h2>&nbsp;&nbsp;@translations.FirstOrDefault(t => t.Id == "LOCALIZATION")?.Text</h2>

        <table class="table" style="border: 2px solid #1C2331;">
            <thead style="background-color: #D6EFFF; color: #1C2331; border: 2px solid #1C2331;">
                <tr>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "DefaultText")?.Text</th>
                    <th style="border: 1px solid #1C2331; text-align: center;">@translations.FirstOrDefault(t => t.Id == "LocalizedText")?.Text</th>
                </tr>
            </thead>
            <tbody>


                @foreach (var localization in ViewBag.Localization)
                {
                    <tr>
                        <td style="border: 1px solid #1C2331;">
                            <span>@localization.English</span>
                        </td>
                        <td style="border: 1px solid #1C2331;">
                            <form method="post" asp-controller="Settings" asp-action="UpdateTranslation">
                                <input type="hidden" name="localizationId" value="@localization.Id" />
                                <input type="text" name="localizedText" value="@localization.Translation" style="width: 500px;" />
                                <button type="submit" class="btn btn-primary">@translations.FirstOrDefault(t => t.Id == "SaveText")?.Text</button>
                            </form>
                        </td>
                    </tr>
                }
            </tbody>
        </table>



    </div>

